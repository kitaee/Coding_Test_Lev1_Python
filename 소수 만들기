from itertools import combinations
import math

def solution(nums):
    answer=0
    case=list(combinations(nums,3))
    for k in range(len(case)):
        if(isPrime(sum(case[k]))):
            answer+=1
    return answer
    
    
def isPrime(n):
    for k in range(2,n):
        if(n%k==0):
            return False
    return True
